        binding="basicHttpBinding" bindingConfiguration="ProductoWSPortBinding"
        contract="ServicioWS.ProductoWS" name="ProductoWSPort" />
      <endpoint address="http://localhost:8080/CyberiaStoreWS_Servidor/AlmaceneroWS"
        binding="basicHttpBinding" bindingConfiguration="AlmaceneroWSPortBinding"
        contract="ServicioWS.AlmaceneroWS" name="AlmaceneroWSPort" />
      <endpoint address="http://localhost:8080/CyberiaStoreWS_Servidor/ProveedorWS"
        binding="basicHttpBinding" bindingConfiguration="ProveedorWSPortBinding"
        contract="ServicioWS.ProveedorWS" name="ProveedorWSPort" />
      <endpoint address="http://localhost:8080/CyberiaStoreWS_Servidor/MarcaWS"
        binding="basicHttpBinding" bindingConfiguration="MarcaWSPortBinding"
        contract="ServicioWS.MarcaWS" name="MarcaWSPort" />
    </client>
  </system.serviceModel>
</configuration>